{
  "name": "legacy-eggjs-ts-boilerplate",
  "version": "0.1.0",
  "description": "legacy-eggjs-ts-boilerplate",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-legacy-eggjs-ts-boilerplate",
    "stop": "egg-scripts stop --title=egg-server-legacy-eggjs-ts-boilerplate",
    "dev": "egg-bin dev",
    "lint": "eslint . --ext .ts --fix",
    "test": "egg-bin test",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "debug": "egg-bin debug",
    "clean": "ets clean"
  },
  "dependencies": {
    "egg": "3.31.0",
    "egg-scripts": "2.17.0"
  },
  "devDependencies": {
    "@types/mocha": "10.0.10",
    "@types/node": "22.17.0",
    "@eggjs/tsconfig": "1.3.3",
    "egg-bin": "5.19.0",
    "egg-mock": "5.10.0",
    "eslint-config-egg": "13.0.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-prettier": "5.5.3",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:IndexXuan/legacy-eggjs-ts-boilerplate.git"
  },
  "author": "IndexXuan",
  "license": "MIT"
}
